package mini_project_09_상속;

public class Hqstore {
	
	protected void whatStoreMenu() {
		System.out.println("본점 가격표입니다.");
		}
	
	protected void jajangmyeon() {
		System.out.println("8,000원 입니다.");
		}
	
	protected void jjamppong() {
		System.out.println("8,000원 입니다.");
		}
	
	protected void tangsuyuk() {
		System.out.println("12,000원 입니다.");
		}
	
	protected void gunmandu() {
		System.out.println("3,000원 입니다.");
		}
	
	protected void gonggibab() {
		System.out.println("1,000원 입니다.");
		}
	
	protected void printMenu() {
		whatStoreMenu();
		jajangmyeon();
		jjamppong();
		tangsuyuk();
		gunmandu();
		gonggibab();
	}
	
}
