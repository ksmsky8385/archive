package test;

public class Test {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
int[] a = {69, 10, 30, 2, 16, 8, 31, 22};
		
		System.out.println("Input Data : 69 10 30 2 16 8 31 22\n");
		
    	int m0 = 0;

        for (int i = 0; i < 8; i++) {
            if (a[i] < a[m0]) {
                m0 = i;
            }
        }
        int n0 = a[0];
        a[0] = a[m0];
        a[m0] = n0;
        
        System.out.println("Sorted Data 1´Ü°è : " + a[0] + " " +a[1] + " " + a[2] + " " + a[3] + " " + a[4] + " " + a[5] + " " + a[6] + " " + a[7]);
        
        
        
	}

}
