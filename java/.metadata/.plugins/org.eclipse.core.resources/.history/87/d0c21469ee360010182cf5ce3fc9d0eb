	String 탕수육 = "12,000";
	String 군만두 = "3,000";
	String 공기밥 = "1,000";
	
	void whatStoreMenu(String 지점) {System.out.println(지점 +" 가격표입니다.");}
	void jajangmyeon(String 자장면) {System.out.println(자장면 + "원 입니다.");}
