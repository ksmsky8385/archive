package mini_project_10_추상클래스_2;

public class CarSpecs {
	
	public String color;
	public String tire;
	public int displacement;
	public String handle;
	
	public void getCarSpecs (String color, String tire, int displacement, String handle) {
		this.color = color;
		this.tire = tire;
		this.displacement = displacement;
		this.handle = handle;
	}
	
}
