package mini_project_09_상속;

public class MainClass {
	public static void main(String[] args) {
		
		Store1 store1 = new Store1("1호점", "7,000", "8,000", "12,000", "판매하지 않음", "1,000");
		
		
		
		store1.whatStoreMenu(store1.지점);
		store1.jajangmyeon(store1.자장면);
		store1.jjamppong(store1.짬뽕);
		store1.tangsuyuk(store1.탕수육);
//		store1.gunmandu(store1.군만두);
		System.out.println("판매하지 않습니다.");
		store1.gonggibab(store1.공기밥);
		
		System.out.println("==========================");
		
		
	}

}
